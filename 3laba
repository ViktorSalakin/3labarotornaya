#include <stdio.h>
#include <string.h>
int prov(char s[], int);
int glas(char s[]);
int soglas(char s[]);
main()
{
	int sog, gl, i, F, cnt;
	char c;
	char st[50];
	sog = gl = F = i = 0;
	memset(st, '\0', 50);
	for(i=0; (c=st[i]=getchar())!=EOF; i++)
	{
		if(c==' '|| c=='.'||c==','||c=='\t'||c=='\n')
		{
			if (prov(st,i) == 0 && F==1)
			{
				if(soglas(st)<glas(st))
					cnt++;
			}
		F=0;
		i=(-1);
		memset(st,'\0',50);
		}
		else
		{
			if(F==0 )
				F=1;
		}
		if (c=='\n')
		break;
	}
	printf("%d =kol",cnt);
	return 0;
}
int glas(char s[])
{
	int i, cnt;
	i = 0;
	while (s[i] != '\0')
	{
	if (s[i] == 'o' || s[i] == 'a' || s[i] == 'e' || s[i] == 'u'||s[i] == 'O' || s[i] == 'A' || s[i] == 'E' || s[i] == 'U'||s[i] == 'I'||s[i] == 'i'||s[i] == 'Y'||s[i] == 'y')
		cnt++;
	i++;
	}
	return cnt;
}
int soglas(char s[])
{
	int i, cnt;
	i = 0;
	while (s[i] != '\0')
	{
	if (s[i] != 'i'&& s[i] != 'o' && s[i] != 'a' && s[i] != 'e' && s[i] != 'u'||s[i] != 'O' && s[i] != 'A' && s[i] != 'E' && s[i] != 'U' && s[i] != 'I'  && s[i] != 'y' && s[i] != 'Y')
		cnt++;
	i++;
	}
	return cnt;
}
int prov(char s[], int m)
{
	int F,i;
	F=0;
	for (i=0; i < m; i++)
	{
		if ((s[i]<'A' || s[i]>'Z') && (s[i]<'a' || s[i]>'z'))
			F=1;
	}
	return F;
}
